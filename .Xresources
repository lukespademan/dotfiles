#define myfg	#cccccc
#define mybg	[75]#222222

URxvt*foreground: myfg
URxvt*background: mybg

URxvt*scrollBar: false
URxvt*scrollTtyKeypress: true

URxvt*font: 	xft:Fira Code:regular:size=11,xft:IBM 3270 Narrow:regular:size=11,xft:Meslo LG S for Powerline:regular:size=11
URxvt*depth: 	32

! pixels for external border
URxvt*externalBorder:     0
! pixels for internal border
URxvt*internalBorder:     0


URxvt.termName: rxvt-unicode-256color
